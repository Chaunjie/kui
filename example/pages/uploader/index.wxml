<k-panel title="基础用法" custom-class="custom-panel">
  <k-uploader 
    imgList="{{ imgList }}" 
    imgWidth="{{ imgWidth }}" 
    total="{{ imgTotal }}"
    preview="{{ preview }}"
    showClose="{{ showClose }}"
    imgCount="{{ imgCount }}"
    bind:remove="removeImg"
    bind:add="addImg"/>
</k-panel>